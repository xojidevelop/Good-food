{
    "manifest_version": 2,
    "version": "0.0.0",
    "name": "about-blank-webext",
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "all_frames": true,
        "match_about_blank": true,
        "run_at": "document_start",
        "js": [
          "content.js"
        ]
      }
    ]
  }